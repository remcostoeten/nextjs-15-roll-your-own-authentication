{
	"editor.fontSize": 16,
	"editor.wordWrap": "on",
	"editor.wordWrapColumn": 80,
	"fontSwitcher.fonts": [
	  "JetBrainsMono",
	  "FiraCode",
	  "Hack",
	  "CascadiaCode",
	  "Iosevka",
	  "MesloLGS NF",
	  "Mononoki",
	  "VictorMono",
	  "SourceCodePro",
	  "UbuntuMono",
	  "RobotoMono",
	  "CodeNewRoman",
	  "ComicShannsMono",
	  "InconsolataLGC",
	  "Agave",
	  "AnonymousPro",
	  "3270",
	  "BigBlueTerm437",
	  "ProFont",
	  "Tinos",
	  "Lekton",
	  "Cousine",
	  "Terminus",
	  "FantasqueSansMono",
	  "SpaceMono"
	],
	"editor.fontFamily": "IBM Plex Mono, Iosevka, Jetbrains mono, Hack, MesloLGS, CascadiaCode, RobotoMono, FiraCode, SourceCodePro, UbuntuMono, Inconsolata, DejaVuSansMono, DroidSansMono, Mononoki, AnonymousPro, VictorMono, NotoMono, LiberationMono, SFMono, CodeNewRoman, Terminus, FantasqueSansMono, Agave, BigBlueTerminal, CamingoCode, SpaceMono, ProFont, Tinos, CourierPrime, Hermit, IBM Plex Mono, IBM Plex Mono, IBM Plex, Roboto, Roboto Mono, Inter, Geist Mono",
	"git.ignoreRebaseWarning": true,
	"workbench.editor.pinnedTabsOnSeparateRow": true,
	"workbench.editor.scrollToSwitchTabs": true,
	"workbench.editor.tabSizing": "fixed",
	"workbench.editor.wrapTabs": true,
	"typescript.updateImportsOnFileMove.enabled": "always",
	"git.enableSmartCommit": true,
	"workbench.panel.showLabels": false,
	"explorer.fileNesting.patterns": {
	  "*.ts": "${capture}.js",
	  "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
	  "*.jsx": "${capture}.js",
	  "*.tsx": "${capture}.ts",
	  "tsconfig.json": "tsconfig.*.json",
	  "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb",
	  "*.sqlite": "${capture}.${extname}-*",
	  "*.db": "${capture}.${extname}-*",
	  "*.sqlite3": "${capture}.${extname}-*",
	  "*.db3": "${capture}.${extname}-*",
	  "*.sdb": "${capture}.${extname}-*",
	  "*.s3db": "${capture}.${extname}-*"
	},
	"workbench.sideBar.location": "right",
	"files.autoSave": "onFocusChange",
	"editor.cursorStyle": "underline-thin",
	"editor.lineHeight": 1.8,
	"terminal.integrated.lineHeight": 1.5,
	"notebook.markdown.lineHeight": 1.5,
	"editor.codeActionWidget.includeNearbyQuickFixes": false,
	"explorer.confirmDelete": false,
	"workbench.colorTheme": "Vercel Theme",
	"workbench.layoutControl.enabled": false,
	"editor.letterSpacing": 1,
}
