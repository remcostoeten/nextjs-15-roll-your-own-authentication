{
  "printWidth": 80,
  "tabWidth": 4,
  "useTabs": true,
  "semi": false,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "always",
  "proseWrap": "preserve",
  "htmlWhitespaceSensitivity": "css",
  "endOfLine": "lf",
  "embeddedLanguageFormatting": "auto",
  "singleAttributePerLine": true,
  "jsxSingleQuote": false,
  "quoteProps": "as-needed",
  "requirePragma": false,
  "insertPragma": false,
  "overrides": [
    {
      "files": [
        "*.json",
        "*.jsonc"
      ],
      "options": {
        "parser": "json"
      }
    },
    {
      "files": [
        "*.tsx",
        "*.ts"
      ],
      "options": {
        "parser": "typescript"
      }
    }
  ]
}